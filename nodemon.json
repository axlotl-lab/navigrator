{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/",
    "dist/",
    "coverage/",
    "*.test.ts"
  ],
  "watch": [
    "src/"
  ],
  "typeRoots": [
    "src/"
  ],
  "execMap": {
    "ts": "node -r ts-node/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,ts"
}